<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.deepexi.com.deepexi.mapper.ProductDeliveryMapper">


    <insert id="createDelivery">

      insert into orderDetails
      (`customer_id`, `id`,`address`,`telephone`,`Dr` )
      value
      (#{delivery.customerId} , #{delivery.id} , #{delivery.address} , #{delivery.telephone} ,'0');

    </insert>


</mapper>